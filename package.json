{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test:client": "react-scripts test --env=jsdom",
    "test:server": "node ./node_modules/mocha/bin/mocha ./server/test.spec.js && npm run coverage:server",
    "server": "node server/app.js",
    "coverage:server": "istanbul cover _mocha server/**/*.spec.js -- --timeout 5000 && istanbul report lcov"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "react-scripts": "0.8.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "supertest": "^2.0.1"
  }
}